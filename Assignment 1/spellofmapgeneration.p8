pico-8 cartridge // http://www.pico-8.com
version 34
__lua__

function _init()
	
	cls()
	print("generating...")
	
	--define variables
	width = 32
	height = 32
	
	--x and y index for map
	yindex = 0
	xindex = 0
	mapstring = "roomStrings.push_back(\""

	for y=0, height-1 do
		for x=0, width-1 do
			tile = mget(x,y)
			
			--this is done to ensure that
			--the blank tile in pico 8
			--is replaced with an all black
			--tile. in the sprite sheet,
			--pico's blank tile is a visible
			--x of pixels.
			if(tile == 0)tile = 5
			
			mapstring = mapstring..tostr(tile)..","
			
		end	
	end
	--send to the clipboard
	printh(mapstring..";"..width..";"..height..";"..";\");", "@clip")

	print("done!")
	
end
